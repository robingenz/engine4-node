{
  "name": "engine4-node",
  "version": "0.0.1",
  "description": "Node.js library for the ENGINE4 API.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Robin Genz <mail@robingenz.dev>",
  "license": "Beerware",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf ./dist",
